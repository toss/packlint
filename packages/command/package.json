    "zod": "3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "fast-check": "^3.3.0",
    "tsdown": "^0.18.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zod-fast-check": "^0.8.0"
  },
  "publishConfig": {
    "access": "public"
