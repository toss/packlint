{
  "name": "@packlint/command",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup ./src/index.ts --format esm --dts",
    "dev": "yarn run build --watch",
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@packlint/core": "workspace:^",
    "@types/ramda": "^0.28.20",
    "clipanion": "^3.2.0-rc.13",
    "ramda": "^0.28.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "fast-check": "^3.3.0",
    "tsup": "^6.5.0",
    "typescript": "4.8.3",
    "vite": "^4.0.2",
    "vitest": "^0.26.1",
    "zod-fast-check": "^0.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
