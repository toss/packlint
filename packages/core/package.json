{
  "name": "@packlint/core",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup ./src/index.ts ./src/testing/index.ts --format esm --dts",
    "dev": "yarn run build --watch",
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/ramda": "^0.28.20",
    "chalk": "^5.2.0",
    "cosmiconfig": "^7.0.1",
    "fast-glob": "^3.2.12",
    "find-up": "^6.3.0",
    "fs-extra": "^10.1.0",
    "ramda": "^0.28.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "fast-check": "^3.3.0",
    "tsup": "^6.5.0",
    "typescript": "4.8.3",
    "vite": "^4.0.2",
    "vitest": "^0.26.1",
    "zod-fast-check": "^0.8.0"
  },
  "peerDependencies": {
    "fast-check": "*",
    "zod-fast-check": "*"
  },
  "peerDependenciesMeta": {
    "fast-check": {
      "optional": true
    },
    "zod-fast-check": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
