{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "prepack": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "version": {
      "dependsOn": ["prepack", "//#test:all", "//#lint:all"],
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["version"],
      "outputs": []
    },
    "//#test:all": {
      "dependsOn": ["@packlint/core#build", "@packlint/command#build"],
      "outputs": []
    },
    "//#lint:all": {
      "outputs": []
    },
    "//#packlint:all": {
      "dependsOn": ["packlint#build"],
      "outputs": []
    }
  }
}
